@model List<EventEase.Models.Event> 

<h1>Events</h1>
<table class="table">
    <thead>
        <tr>
            <th>Event Name</th>
            <th>Event Date</th>
            <th>Venue</th>
            <th>Description</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.EventName</td>
                <td>@item.EventDate.ToShortDateString()</td>
                <td>@item.Venue.VenueName</td>
                <td>@item.Description</td>
            </tr>
        }
    </tbody>
</table>
